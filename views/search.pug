extends layouts/main
include mixin/videoBlock

block content
    h1 #{pageTitle}
    .search__header
        h3 Searching for "#{searchingBy}"
    .search__videos
        if videos.length == 0
            h5 No Videos found
        each item in videos
            +videoBlock({
                videoFile: item["videoFile"],
                title: item["title"],
                views: item["views"],
                id: item[ "id" ]
            })